{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "quotes":["error","single"],
      "space-before-blocks":"error",
      "camelcase":"error",
      "curly":"error",
      "handle-callback-err":"off",
      "space-in-parens":"error"
    }
}
